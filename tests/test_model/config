tgrid: t_start = 0, t_stop = 50_ns, nt = 1001

prop: method = newton, use_mcwf = T

pulse:
* type = file, filename = pulse1.dat, id = 1, time_unit = ns, &
  ampl_unit = unitless

ham: type = matrix, n_surf = 4
* sparsity_model = banded, filename = H0.dat
* sparsity_model = dia, filename = H1.dat, pulse_id = 1

dissipator: type = lindblad_ops, conv_to_superop = F, add_to_H_jump = banded
* filename = L1.dat
* filename = L2.dat

psi:
* type = file, filename = psi0.dat

observables: outfile = pops.dat, exp_unit = unitless, is_real = T, &
  time_unit = ns, type = matrix, n_surf = 4, sparsity_model = banded
* column_label = "<P_1> (q1)", filename = O1.dat
* column_label = "<P_1> (q2)", filename = O2.dat
